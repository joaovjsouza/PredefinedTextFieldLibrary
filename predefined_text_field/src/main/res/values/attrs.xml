<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="PredefinedTextField">
        <attr name="predefined_style" format="enum">
            <enum name="person" value="0" />
            <enum name="email" value="1" />
            <enum name="password" value="2" />
            <enum name="search" value="3" />
        </attr>
        <attr name="hint_text" format="string" />
        <attr name="helper_text" format="string" />
        <attr name="error_text" format="string" />
    </declare-styleable>

</resources>